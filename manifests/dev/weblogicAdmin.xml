<?xml version='1.0'?>
<!DOCTYPE service_bundle SYSTEM "/usr/share/lib/xml/dtd/service_bundle.dtd.1">

<service_bundle type='manifest' name='weblogicAdmin'>

<service
	name='application/weblogic/weblogicAdmin'
	type='service'
	version='1'>

	<dependency name='multi-user'
		grouping='require_all'
		restart_on='error'
		type='service'>
			<service_fmri value='svc:/milestone/multi-user:default'/>
	</dependency>

	<exec_method
		type='method'
		name='start'
		exec='/cpg/3rdParty/scripts/cpg/utils/SMFWeblogicAdmin.sh start'
		timeout_seconds='15' >
	</exec_method>

	<exec_method
		type='method'
		name='stop'
		exec='/cpg/3rdParty/scripts/cpg/utils/SMFWeblogicAdmin.sh stop'
		timeout_seconds='30' >
	</exec_method>

	<instance name='dev10d1' enabled='true'>
		<method_context>
			<method_credential user='dev10' group='devapps' />
		</method_context>
	</instance>

	<instance name='dev10d2' enabled='true'>
		<method_context>
			<method_credential user='dev10' group='devapps' />
		</method_context>
	</instance>

